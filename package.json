{
  "name": "design-ai-next",
  "version": "1.0.0",
  "description": "AI-powered architectural map analysis tool",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy:build": "npm run build && npm run start",
    "deploy:pm2": "pm2 start ecosystem.config.js --env production",
    "deploy:restart": "pm2 restart design-ai",
    "deploy:stop": "pm2 stop design-ai",
    "deploy:logs": "pm2 logs design-ai",
    "deploy:status": "pm2 status",
    "deploy:update": "git pull origin main && npm install && npm run build && pm2 restart design-ai"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "openai": "^4.68.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "architecture",
    "ai",
    "analysis",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "openai",
    "gpt4"
  ],
  "author": "Arash DM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arashdm2020/Desing-AI.git"
  },
  "bugs": {
    "url": "https://github.com/arashdm2020/Desing-AI/issues"
  },
  "homepage": "https://github.com/arashdm2020/Desing-AI#readme"
}
